		"plugin",
		"impersonate",
		"impersonation",
		"user",
		"auth",
		"laravel-package",
		"laravel-plugin"
	],
	"homepage": "https://github.com/hapidjus/laravel-impersonate-ui",
	"license": "MIT",
	"authors": [
